    "@capacitor/status-bar": "8.0.0",
    "@google/genai": "^1.15.0",
    "@ionic/vue": "^8.7.3",
    "@ionic/vue-router": "^8.7.3",
    "@vueuse/core": "^14.1.0",
    "google-spreadsheet": "^5.0.2",
    "ionicons": "^8.0.13",
    "neverthrow": "^8.2.0",
    "pinia": "^3.0.4",
    "streaming-markdown": "^0.2.15",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "vue3-google-login": "^2.0.34",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.1",
