{
  "name": "ai-personal-trainer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "docs": "rm -rf dist docs && npm run build && cp -r dist docs",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.4.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@google/genai": "^1.10.0",
    "@ionic/vue": "^8.6.5",
    "@ionic/vue-router": "^8.6.5",
    "@supabase/supabase-js": "^2.52.0",
    "@vueuse/core": "^13.5.0",
    "ionicons": "^8.0.13",
    "neverthrow": "^8.2.0",
    "pinia": "^3.0.3",
    "streaming-markdown": "^0.2.15",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "^1.6.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.6.0",
    "prettier": "3.6.2",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "~7.0.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  }
